    .col-lg-12.col-sm-12.col-xs-12.col-md-12
      .row.back-color        
        .col-lg-6.col-md-5.col-sm-7.col-xs-10
          h3 Goods and Services Tax
        .col-lg-6.col-md-6.col-sm-4.col-xs-12
          h3.pull-right GSTR Returns
    .clearfix
    .header-style1      
    .header-style2      
    .clearfix05   
    .clearfix05   
    .clearfix05   
     .container-fluid
      .col-lg-10.col-sm-6.col-md-12.col-sm-12.pad-l-0.pad-r-0
