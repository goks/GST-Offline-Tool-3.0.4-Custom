        span.small-size
        span GSTR
      .col-lg-2.col-sm-6.col-md-12.col-sm-12.menufont(ng-controller='transctrl')
        span.language.dropdown(ng-init="selectLang('en')")
          i.fa.fa-globe
          span.dropdown-toggle(data-toggle='dropdown') {{selectedLang}}
          ul.dropdown-menu
            li(ng-repeat='language in language')
              a(ng-click='selectLang(language.cd)', href='') {{language.nm}}
