  body(ng-app='returnsApp', data-ng-controller='transctrl', ng-init="init('returns')")
  header#rtHeader.main-header.fade-ng-cloak
    .container.w100
      .row.branding
        .col-xs-12
          a(href='#/home', title='Goods and Services Tax Home')
            img.logo(src='uiassets/images/emblem.png', alt='Goods and Services Tax Home')
          h1.site-title
            a(href='#/home') Goods and Services Tax Offline Tool V3.0.4
          ul.list-inline.mlinks(ng-if='info && info.fy && info.isUpload=="N"')
            li.gsHdInfo
              a.bl0(href='#/gstr/dashboard', data-ng-bind='info.form')
              a.bl0(href='#/gstr/dashboard', data-ng-bind='info.gstn')
              a.bl0(href='#/gstr/dashboard', data-ng-bind='info.fy')
              a(href='#/gstr/dashboard', data-ng-bind='info.month')
          ul.list-inline.mlinks(ng-if='info && info.fy && info.isUpload=="Y"')
            li.gsHdInfo
              a.bl0(href='#/gstr/upload/dashboard', data-ng-bind='info.form')
              a.bl0(href='#/gstr/upload/dashboard', data-ng-bind='info.gstn')
              a.bl0(href='#/gstr/upload/dashboard', data-ng-bind='info.fy')
              a(href='#/gstr/upload/dashboard', data-ng-bind='info.month')
          ul.list-inline.mlinks(ng-if='info && info.fy && info.isUpload=="E"')
            li.gsHdInfo
              a.bl0(href='#/gstr/error/dashboard', data-ng-bind='info.form')
              a.bl0(href='#/gstr/error/dashboard', data-ng-bind='info.gstn')
              a.bl0(href='#/gstr/error/dashboard', data-ng-bind='info.fy')
              a(href='#/gstr/error/dashboard', data-ng-bind='info.month')
          ul.list-inline.mlinks(ng-if='infoRfd && infoRfd.fy && infoRfd.isUpload=="N"')
            li.gsHdInfo
              a.bl0(href='#/refunds/dashboard', data-ng-bind='infoRfd.form | uppercase')
              a.bl0(href='#/refunds/dashboard', data-ng-bind='infoRfd.gstn')
              a.bl0(href='#/refunds/dashboard', data-ng-bind='infoRfd.fy')
              a(href='#/refunds/dashboard', data-ng-bind='infoRfd.month')
  #rtContainer.content-wrapper
    .dimmer-holder
      #dimmer
    .container.fade-ng-cloak.w100
      .mypage
        div(data-ng-controller='mainctrl', ng-view='')
  footer.offFtr
    #demo.ifooter
      .f2
        .container
          .row
            .col-xs-14
              p Â© 2016-17 Goods and Services Tax Network
